#include <stdio.h>

int main()
{
    int t;
    scanf("%d", &t);
    while (t--)
    {
        int n;
        scanf("%d", &n);
        int arr[n];
        for (int i = 0; i < n; i++)
        {
            scanf("%d", &arr[i]);
        }
        int shifts = 0, comparisons = 0;
        for (int i = 1; i < n; i++)
        {
            int key = arr[i];
            int j = i - 1;
            while (j >= 0 && arr[j] > key)
            {
                arr[j + 1] = arr[j];
                j = j - 1;
                shifts++;
            }
            arr[j + 1] = key;
            comparisons++;
        }
        printf("Shifts: %d \nComparisons: %d\n", shifts, comparisons);
    }
    return 0;
}

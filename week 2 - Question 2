
#include <stdio.h>

int main()
{
    int t;
    scanf("%d", &t);
    
    while (t--)
    {
        int n;
        scanf("%d", &n);
        
        int arr[n];
        for (int i = 0; i < n; i++)
            scanf("%d", &arr[i]);
        
        int i, j, k;
        int found = 0;
        
        for (i = 0; i < n-2; i++)
        {
            for (j = i+1; j < n-1; j++)
            {
                for (k = j+1; k < n; k++)
                {
                    if (arr[i] + arr[j] == arr[k])
                    {
                        printf("%d %d %d\n", i, j, k);
                        found = 1;
                        break;
                    }
                }
            }
        }
        
        if (!found)
            printf("-1\n");
    }
    
    return 0;
}
